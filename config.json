{
	"tokenizer": "transformers.AlbertTokenizer",
	"tokenizer_path": "albert-large-v2",
	"model": "transformers.AlbertForMaskedLM",
        "model_config": "transformers.AlbertConfig",
	"seq_length": 512,
	"layer": 11,
	"n_embed": 1024,
        "pad_token": "<pad>",
        "sep_token": "[SEP]",
        "cls_token": "[CLS]",
        "mask_token": "[MASK]",
	"csv_file": "split/provided.csv",
        "mask_train_file": "split/mask_train.csv",
	"mask_valid_file": "split/mask_valid.csv",
        "mask_field": ["BEFORE_BODY", "AFTER_BODY"],
	"mask_column": ["SEQUENCE1", "SEQUENCE2"],
        "weight_mlm": 1.0,
        "weight_seq": 1.0,
        "mask_ratio": 0.15,
        "batch_size": 3,
	"num_workers": 8,
	"mlm_lr": 3e-5,
	"gamma": 0.7,
	"weight_decay": 0.0,
        "valid_step": 5000
}

{
	"tokenizer": "transformers.AlbertTokenizer",
	"tokenizer_path": "albert-large-v2",
	"model": "transformers.AlbertForMaskedLM",
        "model_config": "transformers.AlbertConfig",
	"seq_length": 512,
	"layer": 11,
	"n_embed": 1024,
        "pad_token": "<pad>",
        "sep_token": "[SEP]",
        "cls_token": "[CLS]",
        "mask_token": "[MASK]",
	"csv_file": "split/provided.csv",
        "mask_train_file": "split/mask_train.csv",
	"mask_valid_file": "split/mask_valid.csv",
        "weight_mlm": 1.0,
        "weight_seq": 1.0,
        "mask_ratio": 0.15,
        "batch_size": 3,
	"num_workers": 8,
	
	"start_lr": 1e-7,
	"T_max": 5,
	"eta_min": 1e-5,
	"base_lr": 1e-7,
	"max_lr": 1e-5,
	"step_size_up": 1,
	"step_size_down": 3,

	"weight_decay": 0.0,
        "scheduler_step": 100,
        "valid_step": 10000,
	"max_iter": 250000,
        "test_file": "split/provided.csv"
}
